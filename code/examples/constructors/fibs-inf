fun next l = 
    case l of
        Nil -> Nil
        Cons a t -> 
            (case t of
                Nil -> Nil
                Cons b d -> 
                    let x = a + b
                        y = call next t
                    in Cons x y);
fun fibs = 
    let x = 0
        y = let 
                z = 1
                w = call next (call fibs)
            in Cons z w
    in  Cons x y;
fun main = call take 3 (call fibs);
fun take n l = 
    case l of 
        Nil -> -2
        Cons a b -> 
            if n then call take (n-1) b else a